# Ansible secrets template
# Copy to secrets.yml and fill in values
# Use ansible-vault to encrypt: ansible-vault encrypt secrets.yml

# NordVPN WireGuard private key
# Get from NordVPN dashboard or existing wg0.conf
wireguard_private_key: "your-wireguard-private-key="

# Tailscale auth key (reusable, with exit node tag)
# Generate at: https://login.tailscale.com/admin/settings/keys
tailscale_authkey: "tskey-auth-xxxxx"
